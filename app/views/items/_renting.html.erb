<section id="content2">
    <div class="row">
      <% @my_bookings.each do |booking| %>
      <div class="col col-xs-12 col-sm-4">
      <div class="card-category"><%= booking.item.name %></div>
          <p><%= booking.start_date %></p>
          <p><%= booking.end_date %></p>
      </div>
      <p><%= link_to "Edit Booking", edit_item_booking_path(booking.item, booking) %></p>
      <p><%= link_to "Delete Booting", item_booking_path(booking.item, booking), method: :delete,
    data: { confirm: "Are you sure you want to delete this booking?" } %></p>
      <% end %>
    </div>
  </div>
</section>
